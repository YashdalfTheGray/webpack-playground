{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": "usage",
      "corejs": {
        "version": "3.6",
        "shippedProposals": true
      },
      "modules": "cjs"
    }],
    "@babel/preset-react"
  ],
  "plugins": ["react-refresh/babel", "@babel/plugin-proposal-class-properties"]
}
